{
  "name": "@bbc/apache2-license-checker",
  "version": "1.1.1",
  "description": "Automated license checker for validating project dependencies for compatable Apache2 licenses.",
  "main": "run.js",
  "scripts": {
    "test": "node run.js",
    "patch": "npm version patch -m 'Upgrade to version %s'",
    "preversion": "npm test",
    "postversion": "git push && git push --tags"
  },
  "bin": "./bin/run",
  "repository": {
    "type": "git",
    "url": "https://github.com/bbc/apache2-license-checker.git"
  },
  "author": "BBC",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/bbc/apache2-license-checker/issues"
  },
  "dependencies": {
    "colors": "^1.1.2",
    "license-checker": "^8.0.4"
  }
}
